function compareSpecificColumnsAndHighlight() {
  // Set the URL of your Google Sheet
  var sheetURL = "YOUR_GOOGLE_SHEET_URL";

  // Set the names of the tabs containing the tables
  var table1TabName = "Table1";
  var table2TabName = "Table2";

  // Set the specific columns to compare (columns are 0-indexed)
  var table1ColumnIndex = 0; // Column A in Table 1
  var table2ColumnIndex = 1; // Column B in Table 2

  // Get the active spreadsheet
  var spreadsheet = SpreadsheetApp.openByUrl(sheetURL);

  // Get the sheets by name
  var table1Sheet = spreadsheet.getSheetByName(table1TabName);
  var table2Sheet = spreadsheet.getSheetByName(table2TabName);

  // Get the data from the specific columns
  var table1Data = table1Sheet.getRange(1, table1ColumnIndex + 1, table1Sheet.getLastRow(), 1).getValues();
  var table2Data = table2Sheet.getRange(1, table2ColumnIndex + 1, table2Sheet.getLastRow(), 1).getValues();

  // Get the maximum number of rows between the two tables
  var numRows = Math.max(table1Data.length, table2Data.length);

  // Compare the columns and highlight inconsistencies
  for (var row = 0; row < numRows; row++) {
    // Check if values are different
    if (table1Data[row] && table2Data[row] && table1Data[row][0] !== table2Data[row][0]) {
      // Highlight the cells with inconsistencies
      table1Sheet.getRange(row + 1, table1ColumnIndex + 1).setBackground("#FFEB3B"); // Set the background color to light yellow
      table2Sheet.getRange(row + 1, table2ColumnIndex + 1).setBackground("#FFEB3B"); // Set the background color to light yellow
    }
  }
}
